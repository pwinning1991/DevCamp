<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title:  "My Portfolio", sticky: false) %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], title:  "My Portfolio", sticky: false) %>
<% else %>
  <%= js add_gritter(flash[:notice], title:  "My Portfolio", sticky: false) %>
<% end %>
